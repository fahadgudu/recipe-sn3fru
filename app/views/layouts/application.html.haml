!!! 5%html	%head		%title Recipe App		= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true		= javascript_include_tag 'application', 'data-turbolinks-track' => true		= csrf_meta_tags		%link{:href => "http://mattscode.io/assets/dist/css/bootstrap.min.css", :rel => "stylesheet"}		%link{:href => "http://mattscode.io/assets/css/animate.css", :rel => "stylesheet"}		%link{:href => "http://fonts.googleapis.com/css?family=Exo+2:300", :rel => "stylesheet", :type => "text/css"}		%link{:href => "http://mattscode.io/assets/css/master.css", :rel => "stylesheet"}	%body		= render partial: 'layouts/header'		.container			- flash.each do |name, msg|				= content_tag :div, msg, class: "alert"			= yield			%hr			%footer				%p Copyright &copy; #{Time.now.year} MarcosSilva.